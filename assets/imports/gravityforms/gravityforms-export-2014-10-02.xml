<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.8.17">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Contact Us]]></title>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[Submit]]></text>
		</button>
		<fields>
			<field allowsPrepopulate="1" id="1" size="medium" type="hidden">
				<inputName><![CDATA[team-member-name]]></inputName>
				<label><![CDATA[Team member to contact]]></label>
			</field>
			<field allowsPrepopulate="1" id="2" size="large" type="text">
				<description><![CDATA[You selected to email this person by selecting them from the team page. If you would like to send to someone else either change your selection or delete the persons name from this field.]]></description>
				<inputName><![CDATA[team-member-name]]></inputName>
				<label><![CDATA[To]]></label>
				<conditionalLogic actionType="show" logicType="all">
					<rules>
						<rule fieldId="1" operator="isnot">
							<value></value>
						</rule>
					</rules>
				</conditionalLogic>
			</field>
			<field id="3" type="name" isRequired="1" size="medium" nameFormat="normal">
				<label><![CDATA[Your Name]]></label>
				<inputs>
					<input id="3.3">
						<label><![CDATA[First]]></label>
					</input>
					<input id="3.6">
						<label><![CDATA[Last]]></label>
					</input>
				</inputs>
			</field>
			<field id="4" type="email" isRequired="1" size="large">
				<label><![CDATA[Email]]></label>
			</field>
			<field id="5" type="text" size="large">
				<label><![CDATA[Website]]></label>
			</field>
			<field id="6" type="textarea" isRequired="1" size="large">
				<label><![CDATA[Your Message]]></label>
			</field>
			<field id="7" type="checkbox" size="medium">
				<choices>
					<choice isSelected="1">
						<text><![CDATA[Send me periodic emails from National Hickman]]></text>
					</choice>
				</choices>
			</field>
		</fields>
		<confirmations>
			<confirmation id="54229c58df9e3" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
			</confirmation>
		</confirmations>
		<notifications>
			<notification id="54229c58d765f">
				<to><![CDATA[{admin_email}]]></to>
				<name><![CDATA[Admin Notification]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[email]]></toType>
				<subject><![CDATA[New submission from {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
				<isActive><![CDATA[0]]></isActive>
			</notification>
		</notifications>
	</form>
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Email Signup]]></title>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[>]]></text>
		</button>
		<fields>
			<field id="1" isRequired="1" size="medium" type="email">
				<label><![CDATA[Email]]></label>
			</field>
		</fields>
		<notifications>
			<notification id="54229c64e4085">
				<to><![CDATA[{admin_email}]]></to>
				<name><![CDATA[Admin Notification]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[email]]></toType>
				<subject><![CDATA[New submission from {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
				<isActive><![CDATA[0]]></isActive>
			</notification>
		</notifications>
		<confirmations>
			<confirmation id="54229c64e4451" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
			</confirmation>
		</confirmations>
		<enableHoneypot><![CDATA[1]]></enableHoneypot>
	</form>
</forms>